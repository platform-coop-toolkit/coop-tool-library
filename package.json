{
  "name": "@platform-coop-toolkit/labor-platform-hub",
  "version": "0.0.0",
  "description": "Practical tools for co-ops.",
  "main": ".eleventy.js",
  "scripts": {
    "build:site": "eleventy",
    "build:assets": "cross-env NODE_ENV=production run-s mix",
    "build": "run-s clean build:*",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf _site",
    "mix": "webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "concurrently \"npm run watch:site\" \"npm run watch:assets\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:site": "eleventy --serve",
    "watch:assets": "cross-env NODE_ENV=development run-s \"mix -- --watch\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/platform-coop-toolkit/labor-platform-hub.git"
  },
  "keywords": [
    "eleventy",
    "netlify-cms",
    "static-site"
  ],
  "author": "OCAD University <platform-coop-development-kit@lists.inclusivedesign.ca>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/platform-coop-toolkit/labor-platform-hub/issues"
  },
  "homepage": "https://github.com/platform-coop-toolkit/labor-platform-hub#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "laravel-mix": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "svelte": "^3.20.1",
    "svelte-loader": "^2.13.6",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "@platform-coop-toolkit/pinecone": "^1.0.0-alpha.13",
    "laravel-mix-svelte": "^0.1.3",
    "node-fetch": "^2.6.0"
  }
}
